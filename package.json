{
  "name": "fastity-example",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "ssh://git@github.com/kinokoruumu/fastity-example.git",
  "author": "Kazuki Nishikawa <kazuki_nishikawa@voyagegroup.com>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p .",
    "start": "node dist/server.js",
    "dev": "npm-run-all -s clean -p build:watch serve",
    "serve": "wait-on dist/server.js && nodemon dist/server.js",
    "clean": "rm -rf dist",
    "build": "npm-run-all -p build:client build:server",
    "build:watch": "npm-run-all -p build:watch:*",
    "build:client": "webpack-cli --config webpack.config.client.js",
    "build:server": "webpack-cli --config webpack.config.server.js",
    "build:watch:client": "yarn build:client --watch",
    "build:watch:server": "yarn build:server --watch",
    "lint": "eslint ./**/*.{ts,tsx}",
    "format": "yarn lint --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-react-inline-elements": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/connect-timeout": "^0.0.34",
    "@types/express": "^4.17.6",
    "@types/history": "^4.7.5",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.12.1",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-redux": "^7.1.7",
    "@types/react-transition-group": "^4.2.4",
    "@types/shallowequal": "^1.1.1",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "axios": "^0.19.2",
    "connect-timeout": "^1.9.0",
    "fastify": "^2.13.0",
    "fastify-static": "^2.6.0",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "shallowequal": "^1.1.0",
    "styled-components": "^5.1.0",
    "sweet-scroll": "^4.0.0",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "universal-router": "^8.3.0"
  },
  "nodemonConfig": {
    "watch": [
      "dist/server.js"
    ],
    "ignore": [
      "**/*.map"
    ],
    "env": {
      "NODE_ENV": "local"
    },
    "ext": "js,json"
  }
}
